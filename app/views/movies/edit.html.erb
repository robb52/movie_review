<div class="col-6 offset-3"> 

  <h1>Editing Movie</h1>

  <div class="card">
    <div class="card-body">

      <%= render 'form', movie: @movie %>

    </div>
  </div>

  <%= link_to 'Back', movies_path, class: "btn btn-primary btn-sm" %>
  <%= link_to 'Delete', movie_path, method: :delete, data: { confirm: "Are you sure?" }, class: "btn btn-danger btn-sm" %>

</div>  

       